@model IEnumerable<AppAgenceVoyage.Models.Gestionnaire>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NomUtilisateur)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PreomUtilisateur)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmailUtilisateur)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TelephoneUtilisateur)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CNIGestionnaire)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Agence.AdresseAgence)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomUtilisateur)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PreomUtilisateur)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmailUtilisateur)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TelephoneUtilisateur)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CNIGestionnaire)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Agence.AdresseAgence)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.IdUtilisateur }) |
                @Html.ActionLink("Details", "Details", new { id = item.IdUtilisateur }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.IdUtilisateur })
            </td>
        </tr>
    }

</table>
